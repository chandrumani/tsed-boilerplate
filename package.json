{
  "name": "image-processor-boilerplate",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "npm run tsc",
    "tsc": "tsc --project tsconfig.compile.json",
    "tsc:w": "tsc --project tsconfig.json -w",
    "start": "tsnd --inspect --ignore-watch node_modules --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js"
  },
  "dependencies": {
    "@tsed/ajv": "^6.56.1",
    "@tsed/common": "^6.56.1",
    "@tsed/core": "^6.56.1",
    "@tsed/di": "^6.56.1",
    "@tsed/exceptions": "^6.56.1",
    "@tsed/json-mapper": "^6.56.1",
    "@tsed/platform-express": "^6.56.1",
    "@tsed/schema": "^6.56.1",
    "@tsed/swagger": "^6.56.1",
    "ajv": "^8.6.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "method-override": "^3.0.0"
  },
  "devDependencies": {
    "@types/bytes": "^3.1.0",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.12",
    "@types/method-override": "0.0.31",
    "@types/multer": "^1.4.5",
    "@types/node": "^15.12.2",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.3.2"
  },
  "tsed": {
    "packageManager": "npm",
    "convention": "default"
  }
}
